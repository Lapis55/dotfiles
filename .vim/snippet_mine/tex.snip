snippet figure1
alias   \figure1 1fig
    \begin{figure}[${1}]
        \centering
        \includegraphics[${2:width=${3\}}]{${4}.${5}}
        \caption{${6}}
        \label{fig.$4}
    \end{figure}${0}

snippet figure2
alias   \figure2 2fig
    \begin{figure}[${1}]
    \begin{minipage}{${2}\hsize}
        \centering
        \includegraphics[${3:width=${4\}}]{${5}.${6}}
        \caption{${7}}
        \label{fig.$5}
    \end{minipage}
    \begin{minipage}{${8}\hsize}
        \centering
        \includegraphics[${9:width=${10\}}]{${11}.${12}}
        \caption{${13}}
        \label{fig.$11}
    \end{minipage}
    \end{figure}${0}


snippet Ass
alias ass \Ass \ass
    \begin{assumption}\label{ass.${1}}
        ${2:TARGET}
    \end{assumption}${0}

